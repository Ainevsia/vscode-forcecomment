{
	"name": "vscode-forcecomment",
	"displayName": "ForceComment",
	"description": "Forcing you to comment from time to time.",
	"version": "0.0.1",
	"publisher": "ainevsia",
	"author": {
        "name": "ainevsia"
    },
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
        "comment"
	],
	"homepage": "https://github.com/Ainevsia/vscode-forcecomment",
	"bugs": {
        "url": "https://github.com/Ainevsia/vscode-forcecomment/issues",
        "email": "zhipengxu@sjtu.edu.com"
	},
	"license": "BSD-3-Clause",
	"activationEvents": [
		"*"
	],
	"repository": {
        "type": "git",
        "url": "https://github.com/Ainevsia/vscode-forcecomment.git"
    },
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.forcecomment.startForceComment",
				"title": "ForceComment: Start forcing you to comment"
			},
			{
				"command": "extension.forcecomment.hitComment",
				"title": "ForceComment: Insert a comment and refresh the timer"
			}
		],
		"keybindings": [
			{
				"command": "extension.forcecomment.hitComment",
				"key": "/",
				"mac": "/",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"minimist": "^1.2.5"
	}
}
